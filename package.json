{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf ./build",
    "modules:prepare": "node --experimental-loader=extensionless ./modulesPrepare.js",
    "modules:update": "node --experimental-loader=extensionless ./modulesUpdate.js",
    "start": "node --experimental-loader=extensionless ./build/index.js",
    "start:ts": "ts-node ./src/index.ts",
    "lint": "eslint \"*.js\""
  },
  "dependencies": {
    "@guessir/backend": "./modules/guessir/packages/backend",
    "@guessir/shared": "./modules/guessir/packages/shared"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "extensionless": "^1.4.5",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
